@model List<SelectListItem>

<div class="row">
    <div class="col s12 center">
        <h4>Registered users</h4>
    </div>
</div>
<div class="row">
    <div class="col m3"></div>
    <div class="col s12 m6">
        <table class="striped responsive-table">
            <thead>
                <tr>
                    <th>Email/Username</th>
                    <th>Role</th>
                    <th>User Id</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var user in Model)
                {
                <tr>
                    <td>
                        <a asp-route="user_update" asp-route-userid="@user.Value">@user.Text.Split("*")[0]</a>
                    </td>
                    <td>
                        <a asp-route="user_update" asp-route-userid="@user.Value">@user.Text.Split("*")[1]</a>
                    </td>
                    <td>
                        <a asp-route="user_update" asp-route-userid="@user.Value">@user.Value</a>
                    </td>
                </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col m3"></div>
</div>
